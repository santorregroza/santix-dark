      "Objects.Blue": "#499df2",
      "Objects.Purple": "#bc8af2",
      "Objects.YellowDark": "#c39c0c",
      "Objects.BlackText": "#181e27",

      "Checkbox.Background.Default.Dark": "#3D465C",
      "Checkbox.Background.Selected.Dark": "#FFA04D",
      "Checkbox.Background.Disabled.Dark": "#181e27",

      "Checkbox.Foreground.Selected.Dark": "#181e27",
      "Checkbox.Foreground.Disabled.Dark": "#3D465C",

      "Checkbox.Border.Default.Dark": "#3D465C",
