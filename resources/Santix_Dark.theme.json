{
  "name": "Santix Dark",
  "dark": true,
  "author": "",
  "editorScheme": "/Santix_Dark.xml",
  "colors": {
    "foreground": "#959BA4",
    "background": "#242936",
    "red": "#f65454",
    "accent": "#FFA04D",
    "secondary": "#3D465C",
    "light": "#dcdcdc",

    "button": "#3D465C",
    "pressed": "#181e27",

    "border": "#181e27",
    "componentBorder": "#181e27",

    "darker": "#181e27",
    "greener": "#3AA87B",

    "panel": "#364357"
